  <style>
        @keyframes bounce {
            0%, 80%, 100% {
                transform: scale(0);
                opacity: 0.3;
            }
            40% {
                transform: scale(1);
                opacity: 1;
            }
        }
    </style>
  <div id="modalLoading" style="display: none;">
    <h4 style="display: none;" id="token" data-value="<%= token %>"></h4>
    <div id="loadingModal" style="
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: rgba(0, 0, 0, 0.7);
        display: flex;
        align-items: center;
        justify-content: center;
        z-index: 9999;
    ">
      <div style="
            display: flex;
            gap: 10px;
        ">
        <div style="
                width: 15px;
                height: 15px;
                background: #fff;
                border-radius: 50%;
                animation: bounce 1.4s infinite ease-in-out both;
            "></div>
        <div style="
                width: 15px;
                height: 15px;
                background: #fff;
                border-radius: 50%;
                animation: bounce 1.4s infinite ease-in-out both;
                animation-delay: 0.2s;
            "></div>
        <div style="
                width: 15px;
                height: 15px;
                background: #fff;
                border-radius: 50%;
                animation: bounce 1.4s infinite ease-in-out both;
                animation-delay: 0.4s;
            "></div>
      </div>
    </div>
  </div>

